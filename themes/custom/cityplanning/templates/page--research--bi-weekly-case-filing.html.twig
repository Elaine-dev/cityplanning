    <!-- Fixed navbar -->
    {% include directory ~ '/partials/header.html.twig' %}
    
    <div  ng-app="app" ng-controller="biweeklyCaseReportCtrl">
    	<!-- title-bar -->
    	<div id="title-bar">
        	<div class="container">
        		<div class="row">
        			<div class="col-md-8 col-xs-12">
        				<h1>{[{pageTitle}]}</h1>
        			</div>
        			
        			<!-- breadcrumb -->
        			
        			<!-- // End breadcrumb -->
        		</div>
        		
        		<div class="bottom" style="padding-top:20px;"></div>
        	</div>
    	</div><!-- //End title-bar -->
    	
    	<div class="clear"></div>
    	
    	<!-- contain -->
    	<div class="container section-padding-inner">
    		<div class="row">
    			<div class="col-md-12 col-xs-12">
    				<!-- hearings -->
    				<div class="padding-bottom20">
    					<div class="row">
        					<div class="col-md-10 col-xs-12">
        						<label>Filter by : </label>
        						<select name="year" ng-model="date">
            						<option>- Year -</option>
            						<option ng-repeat="d in years" ng-selected="d==date" ng-value="d" ng-bind="d" ></option>
            					</select>
        					</div>
    					
    						<div class="col-md-2 col-xs-12 pull-right content-search"><input ng-model="date" placeholder="Search case report"></div>
    					</div>
    				</div>
    				
    				<div class="table-responsive">
    					<table class="table">
                            <colgroup>
                            	<col class="col-md-10">
                              	<col class="col-md-1">
                            </colgroup>
                            
                            <thead>
                                <tr>
                    				<th>Date</th>
                    				<th>File</th>
                    			</tr>
                            </thead>
                            
                            <tr ng-repeat= "record in biweeklyCaseFiling | filter:date" >
                    			<td>{[{ record.date }]}</td>
                    			<td><span ng-if="record.file"><a href="{[{record.file}]}" target="_blank"><i class="fa fa-file-pdf-o fa-2x" aria-hidden="true"></i></a></span></td>
                    		</tr>
                        </table>
    				</div>
    			</div>
    		</div>
    	</div>
	</div>	<!-- //End container -->
			
    <!-- footer section -->
    {{ attach_library ('cityplanning/bi-weekly-case-filing') }}
    {{ attach_library ('cityplanning/angular-js') }}
    {% include directory ~ '/partials/footer.html.twig' %}