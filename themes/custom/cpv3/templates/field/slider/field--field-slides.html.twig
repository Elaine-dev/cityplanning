{% for item in items %}
<div class="swiper-slide">
    {% set url = file_url(item.content['#field_collection_item'].get('field_slide_image').entity.uri.value | image_style('slider_image')) %}

    <img src="{{ url }}"/>
    <div class="slider-content">{{ item.content['#field_collection_item'].get('field_content').value }}</div>
</div>
{% endfor %}
