    <style>
        /* calendar */
        #calendar {max-width: 900px; margin: 0 auto; padding: 0 10px;}
        #top {background: #eee; border-bottom: 1px solid #ddd; padding: 0 10px; line-height: 40px; font-size: 12px;}
        .hearing {background: #eee !important; color: #000 !important; font-size: 12px; border-left: #3399ff solid 2px !important;}
        .commission {background: #eee !important; color: #000 !important; font-size: 12px; border-left: #9966cc solid 2px;}
        .public {background: #eee !important; color: #000 !important; font-size: 12px; border-left: #70a300 solid 2px;}
        .apc {background: #eee !important; color: #000 !important; font-size: 12px; border-left: #0b2c4b solid 2px !important;}
        .mousepointer{cursor:pointer;}

        .cal-hearing { float: left; width: 20px; min-height: 10px; background: #3399ff; padding-top: 19px !important; }
        .cal-hearing-disable { float: left; width: 20px; min-height: 10px; background-color: #fff; border: 1px solid #aed5fc; padding-top: 19px !important; }

        .cal-commission { float: left; width: 20px; min-height: 10px; background: #9966cc; padding-top: 19px !important;}
        .cal-commission-disable { float: left; width: 20px; min-height: 10px; background-color: #fff; border: 1px solid #bfaad4; padding-top: 19px !important;}

        .cal-apc { float: left; width: 20px; min-height: 10px; background: #0b2c4b; padding-top: 19px !important;}
        .cal-apc-disable { float: left; width: 20px; min-height: 10px; background-color: #fff; border: 1px solid #09233c; padding-top: 19px !important;}
        
        .cal-community-events { float: left; width: 20px; min-height: 10px; background: #70a300; padding-top: 19px !important;}
        .cal-community-events-disable { float: left; width: 20px; min-height: 10px; background-color: #fff; border: 1px solid #f2ae41; padding-top: 19px !important;}

        .cal-zah { float: left; width: 20px; min-height: 10px; background: #ffb845; padding-top: 19px !important;}
        .cal-zah-disable { float: left; width: 20px; min-height: 10px; background-color: #fff; border: 1px solid #f2ae41; padding-top: 19px !important;}
        
        .cal-ddah { float: left; width: 20px; min-height: 10px; background: #0192d1; padding-top: 19px !important;}
        .cal-ddah-disable { float: left; width: 20px; min-height: 10px; background-color: #fff; border: 1px solid #0289c3; padding-top: 19px !important;}

        .cal-box { float: left; width: 20px; min-height: 10px; border: solid 1px #ddd; background:#fff; padding-top: 19px !important;}
        .mousepointer:hover {background-color: #eee;}
        /*.cal-section{padding: 0 !important; }*/
        #ci-public,#ci-commission, #ci-hearing {cursor: pointer;}
        
        .fc-center h2{color:#9ea3a9 !important; font-size:28px !important;}
		#cal-hearing { float: left; width: 20px; min-height: 10px; background: #525f7c; padding-top: 19px !important; }
        #cal-commission { float: left; width: 20px; min-height: 10px; background: #3474e0; padding-top: 19px !important;}
        #cal-public-meeting { float: left; width: 20px; min-height: 10px; background: #70a300; padding-top: 19px !important;}
     	.fc-other-month .fc-day-number { display:none;} 		 /* hide past and future dates */
     	#cal-index{font-size: 12px;}	
     	div.fc-scroller.fc-day-grid-container {
     		overflow-x: auto;
			overflow-y: auto;
     	}
    </style>


