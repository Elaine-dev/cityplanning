	{# Create json file when page is upload  #}
	{{ drupal_block('lascea_block') }}

	<!-- contain -->
	<div class="row">
		<div class="col-md-12">
			<div ng-app="appSCEA" ng-controller="sceaCtrl" class="ng-scope" id="eirs">
				<div class="row">
					<div class="col-md-12">
            			<div ng-disabled="enable=='false'">
                            <center ng-show="loadingText == 'Loading ...'">
                            	<i class="fa fa-spinner fa-spin fa-2x fa-fw"></i><span class="sr-only">Loading...</span>
                            </center>
                        </div>
            		</div>
				</div>

				<div class="row">
        			<div class="col-md-12 padding-left0 padding-right0">
        				<div class="table-responsive">
                        	<table class="table table-hover">
                            	<colgroup>
                                    <col class="col-md-3">
                                    <col class="col-md-6">
                                    <col class="col-md-1">
                                </colgroup>

                                <thead>
                                    <tr>
                                        <th>Case No.</th>
                                        <th>Project Title</th>
                                        <th></th>
                                    </tr>
                                </thead>

                                <tbody>
                                    <tr ng-repeat="x in data | filter:projectTitle | startFrom:(currentPage - 1) * pageSize | limitTo: pageSize" class="ng-scope">
                                        <td ng-bind="x.case_number"></td>
                                        <td ng-bind="x.projectTitle"></td>
                                        <td ngIf: x.default>
                                            <a href="{[{ x.default }]}" ng-if="x.default" class="ng-scope">
                                            	<i class="fa fa-file-code-o fa-lg" aria-hidden="true"></i>
                                        	</a>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>                        

                        <div class="bottom "></div>

                	</div>
                </div>
			</div>
		</div>
	</div> <!-- //End container -->

	{{ attach_library ('cpv3/scea-js') }}
	{{ attach_library ('cpv3/angular-js') }}

