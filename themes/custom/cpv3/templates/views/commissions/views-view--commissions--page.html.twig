            <div class="row">
              <div class="col-md-12" id="teams">
                {% if header %}
                  <header class="padding-bottom25">
                    {{ header }}
                  </header>
                {% endif %}

                <div class="padding-bottom60">
                  <a href="#"><img src="{{ theme_base_path }}/images/i_accessUpcomingAgendas.jpg" border="0" class="padding10"></a>
                  <a href="/about/commission-meetings"><img src="{{ theme_base_path }}/images/i_commissionMeetings.jpg" border="0" class="padding10"></a>
                </div>
                			
    			<div id="commissions-tab">
                  <ul class="nav nav-tabs">
                    {% for item in category %}
                      {% if loop.index == 1 %}
                        {% set active_class = 'active'  %}
                        {% set class = 'padding-left0'  %}
                      {% else %}
                        {% set active_class = ''  %}
                        {% set class = ''  %}
                      {% endif %}
                      <li class="padding-top0 {{ active_class }} {{ (loop.last) ? '' : 'tab-line'  }} js-tab">
                        <a data-toggle="tab" id="{{ item.id }}" href="#{{ item.counter }}" class="{{ class }}">
                          {{ item.first_name}}</br>
                          {{ item.last_name }}
                        </a>
                      </li>
                    {% endfor %}
                    </ul>
                    
                    {# display Rules & Procedures link #}
                    {% for item in category %}
                       <div class="paddingtop15 item-{{ item.id }} hidden js-desc">{{ item.description|raw }}</div>
                    {% endfor %}

                    <div class="loading hidden"></div>

                    <div class="tab-content paddingtop15" id="js-content">
                      <div class="tab-pane fade in active">
                        <!-- display content -->
                        {# 
                            display Rules & Procedures link 
                            set first index to display content                        
                        #}
                        {{ category.0.description|raw }}
                        
                        {{ rows }}
                        {{ empty }}
                      </div>
                    </div>
                </div><!-- //End #commissions-tab -->
                    
              </div><!-- //End #team -->
            </div>
