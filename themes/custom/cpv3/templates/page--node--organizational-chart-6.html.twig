 {# page template #}
<!-- Fixed navbar -->
{% include directory ~ '/partials/header.html.twig' %} {# page-banner #}
<div class="container-fluid">
	<div class="row">
		<div class="col-md-12">{{ page.top_banner }}</div>
	</div>
</div>
{# End page-banner #}

<!-- contain -->
<div class="container" id="main-container">
	{# Main content #}
	<div class="row">
		<div class="col-md-12" style="padding-left: 30px;">{{ page.title_bar }}</div>
	</div>

	<div class="row" style="padding-top: 35px; padding-bottom: 35px;">

		<div class="col-md-9 col-md-offset-3 col-centered">
			<div class="row">
				<div class="col-md-12 ">

					<div id="org-slider">
						<div class="main-section" ng-app="myApp" ng-controller="myController">

							<div class="leaf leaf-1" style="padding-top: 25px; padding-bottom: 25px;">

								<!--main loop-->
								<div ng-repeat="member in list">
									<div class="boss-card2">
										<div class="row" style="padding: 25px;">
											<div class="col-md-2 col-xs-2">
												<img src="{{ theme_base_path }}/images/city-planning-bureaus/{[{member.avatar}]}" class="img-responsive img-circle" style="margin-top: -10;">
											</div>
											<div class="col-md-8 col-xs-8"">
												<h4 class="card-title">{[{member.name|camelize}]}</h4>
												<p class="card-text">{[{member.title}]}</p>
											</div>
										</div>
									</div>

									<!--first child loop-->
									<div class="child-card" ng-repeat="child in member.children">
										<div class="row">
									       <div class="col-md-2 col-xs-2">
	                                            <img src="{{ theme_base_path }}/images/city-planning-bureaus/{[{child.avatar}]}" class="img-responsive img-circle">
	                                        </div>
	                                        <div class="col-md-8 col-xs-8"">
	                                            <h4 class="card-title">{[{child.name|camelize}]}</h4>
	                                            <p class="card-text">{[{child.title}]}</p>
	                                        </div>
	                                        <div class="col-md-2 col-xs-2 icon next" data-children="" data-filter="{[{child.name}]}" data-parent="">
	                                            <i class="icon fa fa-angle-right fa-3x"></i>
	                                        </div>
										</div>
									</div>
								</div>
							</div>

							<div class="leaf leaf-2 scrollStyle" ng-repeat="n in [] | range:10">
								<div class="row" style="padding-left: 0; padding-right: 0;">
									<div class="col-md-6" style="margin: 0; padding: 0;">
										<div class="card-head" style="padding-top: 15px;">
											<div class="row" style="padding-left: 0;">
												<div class="col-md-2 icon-prev">
													<span class="p-prev-icon"></span>
												</div>

												<div class="col-md-2" style="padding-left: 0; padding-top: 10px;">
													<div class="avatar small-avatar">
														<img class="p-avatar" src="" width="60" height="60">
													</div>
												</div>

												<div class="col-md-8" style="text-align: left; text-transform: capitalize;">
													<h4 class="p-name card-list-fullname"></h4>
													<p class="p-title card-text"></p>
												</div>

											</div>
										</div>
									</div>

									<div class="col-md-6 child-info"></div>

								</div>
							</div>

						</div>
					</div>


				</div>
			</div>

			<div class="row" style="padding: 5px 0 0 0;">
				<div class="col-md footer-background">
					<a href="{{ base_path }}about/organizational-chart-6"
						style="margin-left: 50%;"><i class="fa fa-home fa-2x"
						aria-hidden="true" style="color: #999; padding-top: 5px;"></i></a>
				</div>
			</div>

		</div>

	</div>
</div>
{# End container #}


<!-- footer section -->
{{ attach_library('cpv3/organizational-chart6') }}
{{ attach_library('cpv3/angular-js') }}
{% include directory ~ '/partials/footer.html.twig' %}