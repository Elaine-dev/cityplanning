<!-- Fixed navbar -->
{% include directory ~ '/partials/header.html.twig' %}

<div class="container-fluid">
  <div class="row">

    <div class="col-md-12">
      <div id="page-title-bg"><img src="{{ file_url(node.field_image.entity.fileuri) }}" class="img-responsive " alt="bg"></div>
    </div>
  </div>
</div>
<!-- //End page-banner -->

<!-- contain -->
<div class="container" id="main-container">
  <!-- Main content -->
  <div class="row">
    <div class="col-md-12">
      <h1>{{ node.label }}</h1>
    </div>
  </div>

  <div class="row">
    <!-- Left-side bar -->
    <div class="col-md-9">
      <div class="section-content clear">

        <div class="row padding-left0">
          <div class="col-md-12">

            {% if node.field_slider %}
              {% for parag_id in node.field_slider.value %}
                {{ bamboo_render_entity('paragraph', parag_id['target_id']) }}
              {% endfor %}
            {% endif %}
          </div>
        </div>
        {# menu #}
        <div class="row padding-left0">
          <div class="col-md-12">
            <div id="hollywood-tab">
              <ul class="nav nav-tabs" role="tablist">
                {% for item in hollywood_menu %}
                  {% if loop.index == 1 %}
                    {% set active_class = 'active' %}
                    {% set class = 'padding-left0'  %}
                  {% else %}
                    {% set active_class = '' %}
                    {% set class = '' %}
                  {% endif %}
                  <li role="presentation" class="padding-top0 {{ (loop.last) ? '' : 'tab-line' }} {{ active_class }}">
                    <a href="#{{ item.id }}" aria-controls="{{ item.counter }}" role="tab" data-toggle="tab" class="{{ class }}">
                      {{ item.name }}
                    </a>
                  </li>
                {% endfor %}
              </ul>

              <!-- //End hollywood-tab -->
              <div class="tab-content">
                <div class="tab-pane active fade in row padding-left0" id="218">
                  {% if node.field_tab_about_content_items %}
                    {% for parag_id in node.field_tab_about_content_items.value %}
                      {{ bamboo_render_entity('paragraph', parag_id['target_id']) }}
                    {% endfor %}
                  {% endif %}

                </div>

                <div class="tab-pane fade row padding-left0" id="219">

                  {% if node.field_tab_plan_content_items %}
                    {% for parag_id in node.field_tab_plan_content_items.value %}
                      {{ bamboo_render_entity('paragraph', parag_id['target_id']) }}
                    {% endfor %}
                  {% endif %}

                </div>

                <div class="tab-pane fade row padding-left0" id="220">

                  {% if node.field_tab_map_content_items %}
                    {% for parag_id in node.field_tab_map_content_items.value %}
                      {{ bamboo_render_entity('paragraph', parag_id['target_id']) }}
                    {% endfor %}
                  {% endif %}
                </div>

                <div class="tab-pane fade row padding-left0" id="221">
                  {% if node.field_tab_resources_content_item %}
                    {% for parag_id in node.field_tab_resources_content_item.value %}
                      {{ bamboo_render_entity('paragraph', parag_id['target_id']) }}
                    {% endfor %}
                  {% endif %}

                </div>

                <div class="tab-pane fade row padding-left0" id="222">

                  {% if node.field_tab_contact_content_items %}
                    {% for parag_id in node.field_tab_contact_content_items.value %}
                      {{ bamboo_render_entity('paragraph', parag_id['target_id']) }}
                    {% endfor %}
                  {% endif %}
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- //End Left-side bar -->

    <!-- Right-side bar -->

    <div class="col-md-3 sidebar-menu hidden-sm hidden-xs">
      <div class="top-bar"></div>
      {{ page.side_menu }}
    </div>
    <!-- //End Right-side bar -->

    <!-- //End container -->

    {% for item in full_width_data %}
      {% if item.flag %}
        <div class="clear full-width-content {% if item.para_id == 122 %} block-qoute-wrapper {% endif %}">
          {# full width content goes here #}
          {{ bamboo_render_entity('paragraph', item.para_id) }}
        </div>
      {% else %}
        {# small width content goes here #}
        <div class="container">
          <div class="row cms-fullwidth">
            <div class="section-content clear">
              {{ bamboo_render_entity('paragraph', item.para_id) }}
            </div>
          </div>

        {% endif %}
      {% endfor %}
    </div>
  </div>
</div>
{{ attach_library('cpv3/cpu') }}
<!-- footer section -->
{% include directory ~ '/partials/footer.html.twig' %}