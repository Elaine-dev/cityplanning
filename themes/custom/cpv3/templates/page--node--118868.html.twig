    <!-- Fixed navbar -->
    {% include directory ~ '/partials/header.html.twig' %}
    <div ng-app="app" ng-controller="agendaCtrl">

        {# page-banner #}
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <div id="page-title-bg"><img src="{{ theme_base_path }}/images/page-bg.jpg" class="img-responsive " alt="bg"></div>
                </div>
            </div>
        </div>{# End page-banner #}

        <!-- contain -->
        <div class="container" id="main-container">
            {# Main content #}
            <div class="row">
                {# Left-side bar #}
                <div class="col-md-9">
                    <div class="row padding-left0">
                        <div class="col-md-12">
                        	<!-- title-bar -->
                        	<h1>Agendas & Minutes - {[{ planningAreaTitle }]}</h1>
                        	<!-- //End title-bar -->
                            <div ng-disabled="enable=='false'">
            					<center ng-show="loadingText == 'Loading ...'"><i class="fa fa-spinner fa-spin fa-2x fa-fw"></i><span class="sr-only">Loading...</span></center>
            				</div>

            				<!-- agenda -->
            				<div class="padding-bottom20">
            					<div class="row">
                					<div class="col-md-9 col-xs-12 padding-left0">
                						<label>Filter by : </label>
                    					<select name="apc" ng-model="apc">
                    						 <option ng-repeat="option in apcAreas.availableOptions" value="{[{option.id}]}" ng-selected="apc == option.id">{[{option.name}]}</option>
                    					</select>

                    					<select name="date" ng-model="date">
                    						<option>- Year -</option>
                    						<option ng-repeat="d in dates" ng-selected="d==date" ng-value="d" ng-bind="d" ></option>
                    					</select>
                					</div>

            						<div class="col-md-3 col-xs-12 pull-right content-search"><input ng-model="Date" placeholder="Search your agenda"></div>
            					</div>
            				</div>

            				<div class="table-responsive">
            					<table class="table">
                                    <colgroup>
                                        <col class="col-md-1">
                                        <col class="col-md-2">
                                        <col class="col-md-1">
                                        <col class="col-md-1">
                                        <col class="col-md-4">
                                        <col class="col-md-1">
                                        <col class="col-md-1">
                                        <col class="col-md-1">
                                        <col class="col-md-1">
                                    </colgroup>
                                    <thead>
                                        <tr>
                                            <th>Date</th>
                                            <th>Type</th>
                                            <th>Board Name</th>
                                            <th>Note</th>
                                            <th>Address</th>
                                            <th>Agenda</th>
                                            <th>Audio</th>
                                            <th>Minutes</th>
                                        </tr>
                                    </thead>
                                    <tr ng-repeat="agenda in agendas | filter:Date">
                                        <td ng-bind="agenda.Date"></td>
                                        <td ng-bind="agenda.Type"></td>
                                        <td ng-bind="agenda.BoardName"></td>
                                        <td ng-bind="agenda.Note"></td>
                                        <td ng-bind="agenda.Address"></td>
                                        <td><span ng-if="agenda.AgendaLink"><a href="{[{agenda.AgendaLink}]}" target="_blank"><i class="fa fa-file-pdf-o fa-2x" aria-hidden="true"></i></a></span></td>
                                        <td><span ng-if="agenda.AudioLink"><a href="{[{agenda.AudioLink}]}" target="_blank"><i class="fa fa-file-audio-o fa-2x" aria-hidden="true"></i></a></span></td>
                                        <td><span ng-if="agenda.MinutesLink"><a href="{[{agenda.MinutesLink}]}" target="_blank"><i class="fa fa-file-text-o fa-2x" aria-hidden="true"></i></a></span></td>
                                    </tr>
                                </table>
            				</div>

                        </div>
                    </div>
                </div>{# End Left-side bar #}

                {# Right-side bar #}
                <div class="col-md-3 sidebar-menu padding-right-27">
                    <div class="top-bar"></div>
                     {{ page.side_menu }}
                </div>{# End Right-side bar #}
            </div>
        </div>{# End container #}
    </div>

    <!-- footer section -->
    {{ attach_library ('cpv3/agenda-hearing') }}
    {{ attach_library ('cpv3/angular-js') }}
    {% include directory ~ '/partials/footer.html.twig' %}