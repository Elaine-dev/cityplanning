    <!-- Fixed navbar -->
    {% include directory ~ '/partials/header.html.twig' %}
    
    <style>
        .advance-search-block{background: #d4d9dd; min-height: 180px; padding: 3px 5px 5px 5px;}
        .advance-search-block h2{color: #4d4e4e !important; font-size: 26px !important; font-weight: 600; }
        .advance-search-block p {margin-bottom: 10px !important; font-size: 15px; }
        .advance-search-block-button{width:100%; height: 40px; line-height: 2;}
        .advance-search-block-form{width: 95%; padding: 5px 5px 0 5px; }
        
        .borderless td, .borderless th {
            border: none !important;
        }
    </style>
    
    <div ng-app="searchApp">
        {# page-banner #}
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <div id="page-title-bg" style="background-image: url('{{ theme_base_path }}/images/page-bg.jpg')"></div>
                </div>
            </div>
        </div>{# End page-banner #}

            <!-- contain -->
        <div class="container" id="main-container">
            {# Main content #}
            <div class="row">
                <div class="col-md-12">{{ page.title_bar }}</div>
            </div>
            
            <div class="row content-wrapper">
                <div class="col-md-12">
                    <div class="row padding-left0">                        
                        <div class="col-md-12">
                              <div class="row" style="padding-bottom: 15px;padding-top: 15px;">
                                    <div class="col-md-4" style="padding-left: 0px;">
                                       <div class="advance-search-block">
                                           <h2 class="text-center">CASES</h2>
                                           <center>
                                                <form class="form advance-search-block-form">
                                                    <div class="form-group">
                                                        <label for="address" class="sr-only">Case number</label>
                                                        <input type="text" id="adv-case-number" class="form-control" placeholder="Example: ZA-2011-3269">
                                                    </div>
                                                    <div class="form-group">
                                                        <button type="submit" class="btn btn-z-search advance-search-block-button" id="adv-case-search">Search</button>
                                                   </div>
                                                </form>
                                            </center>
                                        </div>
                                    </div>
                                    
                                    <div class="col-md-4" style="padding-left: 0px;">
                                       <div class="advance-search-block">
                                           <h2 class="text-center">ORDINANCES</h2>
                                           <center>
                                                <form class="form advance-search-block-form">
                                                    <div class="form-group">
                                                        <label for="address" class="sr-only">Case number</label>
                                                        <input type="text" id="ordinances" class="form-control" placeholder="Example: ORD-127564">
